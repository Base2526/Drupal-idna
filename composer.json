{
    "require": {
        "php": ">=5.3.0",
        "monolog/monolog": ">=1.6.0,<2.0.0",
        "graylog2/gelf-php": "~1.5",
        "phpmailer/phpmailer": ">=5.2.21",
        "php-ffmpeg/php-ffmpeg": "*"
    },
    "config": {
        "autoloader-suffix": "ComposerManager",
        "vendor-dir": "../../../all/vendor"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-0": {
            "Drupal\\Monolog": [
                "../../modules/monolog/src"
            ]
        }
    }
}
